<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Primary Colors -->
    <color name="colorPrimary">@android:color/system_accent1_100</color>
    <color name="colorOnPrimary">@android:color/system_accent1_900</color>
    <color name="colorPrimaryContainer">@android:color/system_accent1_700</color>
    <color name="colorOnPrimaryContainer">@android:color/system_accent1_100</color>

    <!-- Secondary Colors -->
    <color name="colorSecondary">@android:color/system_accent2_300</color>
    <color name="colorOnSecondary">@android:color/system_accent2_900</color>
    <color name="colorSecondaryContainer">@android:color/system_accent2_700</color>
